{
  "version": 1,
  "rules": [
    {
      "name": "no-proxy-pass-with-fastcgi-pass",
      "type": "conflictInContext",
      "description": "Checks that one of proxy_pass or fastcgi_pass is in a context",
      "module": "ngx_http_proxy_module",
      "match": {
        "directive": "proxy_pass",
        "params": {
          "other": "fastcgi_pass"
        }
      },
      "action": {
        "severity": "warning",
        "links": [
          "http://nginx.org/en/docs/http/ngx_http_proxy_module.html#proxy_pass",
          "https://docs.nginx.com/"
        ]
      }
    }
  ]
}